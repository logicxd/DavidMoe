<main id="home-body">
    <div id="profile-background"></div>
    <div class="container">

        <div id="profile-header">
            <div id="profile-description-group">
                <img id="profile-picture" class="responsive-img" src="img/index/profile-picture.jpg" alt="profile-picture">
                <h3 id="profile-name">Aung Moe</h3>
                <div id="profile-job" style="font-size: 25px;">Software Engineer</div>
                <div class="row-m-b-0 center">
                    <h1 id="profile-title-message">
                        {{#each data.occupations}}
                        <div id="states-{{@index}}" class="states" data-value="{{this.state}}">
                            {{this.state}}
                        </div>
                        <div id="occupations-{{@index}}" class="occupations" data-value="{{this.occupation}}">
                            {{this.occupation}}
                        </div>
