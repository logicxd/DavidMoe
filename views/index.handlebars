        <div class="divider"></div>

        <div id="education">
            <h3>Education</h3>

            <div class="row">
                <div class="col s3 m2 l1">
                    <img src="img/index/education-uci.png" class="responsive-img">
                </div>
                <div class="col s9 m7 l8">
                    <h5 class="m-t-0">University of California, Irvine</h5>
                    <h6 class="show-on-small">2016 - 2018</h6>
                    <h6>Bachelor of Science in Computer Science</h6>
                    <h6>GPA 3.75 - Cum Laude</h6>
                </div>
                <div class="col s0 m3 l3 hide-on-small-only">
                    <h5 class="right-align education-year">
                        2016 - 2018
                    </h5>
                </div>
            </div>
            <div class="row">
                <div class="col s3 m2 l1">
                    <img src="img/index/education-csm.jpg" class="responsive-img">
                </div>
                <div class="col s9 m7 l8">
                    <h5 class="m-t-0">College of San Mateo</h5>
                    <h6 class="show-on-small">2013 - 2016</h6>
                    <h6>Associate of Science in Computer &amp; Information, Mathematics, and Physics</h6>
                    <h6>GPA 3.65 (3.90 CS) - Magna Cum Laude</h6>
                </div>
                <div class="col s0 m3 l3 hide-on-small-only">
                    <h5 class="right-align education-year">
                        2013 - 2016
                    </h5>
                </div>
            </div>
        </div>
        <div class="divider"></div>

        <div id="projects">
            <h3>Projects</h3>
            {{#each data.projects}}
             <div class="project-row">
                <div class="col s12 m6">
                    <img class="materialboxed projects-image" src="{{this.image}}">
                </div>
                <div class="col s12 m6 projects-description-container">
                    <h5>{{this.title}}</h5>
                    {{#each this.skills}}
                    <div class="chip">
                        <img src="{{this.image}}" alt="Skill Icon"> {{this.name}}
                    </div>
                    {{/each}}
                    <p>
                        {{{this.description}}}
                    </p>
                    <div class="projects-description-button-container">
                        {{#if this.github }}
                        <a href="{{this.github}}" target="_blank" class="waves-effect waves-light btn projects-description-button">
                            <i class="fab fa-github"></i> GitHub</i>
                        </a>
                        {{/if}}
                        {{#if this.website }}
                         <a href="{{this.website.link}}" target="_blank" class="waves-effect waves-light btn projects-description-button">
                            <i class="fas fa-link"></i> {{this.website.name}}</i>
                        </a>
                        {{/if}}
                    </div>
                </div>
            </div>
            {{/each}}
        </div>
