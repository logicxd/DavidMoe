<main>
    <div class="container">
        <h2>Blogs</h2>
        <ul class="blogs-list">
            {{#each pages}}
            <li>
                <span class="blogs-date">{{this.dateString}} - {{this.category}}</span>
                <h4 class="m-t-0">
                    <a class="blogs-link" href="blogs/{{this.url}}">
                        {{this.title}}
                    </a>
                </h4>
            </li>
            {{/each}}
        </ul>
    </div>
</main>