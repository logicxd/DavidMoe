<main>
    <div class="container-blog">
        <h1 id="header" class="m-b-0 scrollspy">{{page.title}}</h1>
        <div class="blog-date">{{page.dateString}} - {{page.category}} - <i class="far fa-clock"></i> {{page.timeToRead}} minute read</div>
        <div class="blog-content flow-text">
            {{{page.html}}}
        </div>
        <div id="disqus_thread"></div>
    </div>
</main>
<script>
    var disqus_config = function () {
        this.page.identifier = '{{{page.url}}}';
    };
</script>